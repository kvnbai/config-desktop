#============= neverallow ==============

neverallow { user_sudo_t user_t system_cronjob_t sysadm_sudo_t } fixed_disk_device_t:blk_file { append read write };
neverallow { user_sudo_t } security_t:security load_policy;
neverallow { user_sudo_t } policy_config_t:file relabelto;
neverallow { user_t } memory_device_t:chr_file read;
neverallow { user_t } security_t:security { load_policy setenforce };
neverallow { user_t } scsi_generic_device_t:chr_file { append write read };
neverallow { user_t } self:capability sys_module;
neverallow { user_t } self:memprotect mmap_zero;
neverallow { user_t } shadow_t:file { create write relabelto };

dontaudit sysadm_sudo_t fixed_disk_device_t:blk_file { append read write };
dontaudit system_cronjob_t fixed_disk_device_t:blk_file { append read write };
dontaudit user_sudo_t fixed_disk_device_t:blk_file { append read write };
dontaudit user_sudo_t policy_config_t:file relabelto;
dontaudit user_sudo_t security_t:security load_policy;
dontaudit user_t fixed_disk_device_t:blk_file { append read write };
dontaudit user_t memory_device_t:chr_file read;
dontaudit user_t security_t:security { load_policy setenforce };
dontaudit user_t scsi_generic_device_t:chr_file { append write read };
dontaudit user_t self:capability sys_module;
dontaudit user_t self:memprotect mmap_zero;
dontaudit user_t shadow_t:file { create write relabelto };

